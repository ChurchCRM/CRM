{
    "name": "churchcrm/crm",
    "version": "5.5.0",
    "type": "project",
    "keywords": [
        "church",
        "crm"
    ],
    "homepage": "https://churchcrm.io",
    "license": "MIT",
    "authors": [
        {
            "name": "George Dawoud",
            "homepage": "http://blog.dawouds.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "email": "info@churchcrm.io",
        "issues": "https://github.com/ChurchCRM/CRM/issues",
        "irc": "https://gitter.im/ChurchCRM/CRM",
        "source": "https://github.com/ChurchCRM/CRM",
        "docs": "https://github.com/ChurchCRM/CRM/wiki"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-PDO": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-gettext": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-session": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "azuyalabs/yasumi": "^2.4.0",
        "defuse/php-encryption": "^2.3.1",
        "drewm/mailchimp-api": "^2.5.4",
        "endroid/qr-code": "^5.0.4",
        "geocoder-php/bing-maps-provider": "^4.2.0",
        "geocoder-php/google-maps-provider": "^4.6.0",
        "ifsnop/mysqldump-php": "^v2.9",
        "knplabs/github-api": "^3.12.0",
        "monolog/monolog": "^2.3.0",
        "mustache/mustache": "^v2.13.0",
        "php-http/guzzle7-adapter": "^1.0.0",
        "php-http/message": "^1.11.1",
        "phpmailer/phpmailer": "^6.5.1",
        "pragmarx/google2fa": "^8.0.0",
        "propel/propel": "2.0.0-alpha12",
        "setasign/fpdf": "^1.8.3",
        "slim/http-cache": "^1.1.0",
        "slim/php-view": "^3.2.0",
        "slim/slim": "^4.12.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/translation": "^5.4.31",
        "verot/class.upload.php": "^2.1.0",
        "vonage/client": "^2.4.0"
    },
    "autoload": {
        "classmap": [
            "ChurchCRM/"
        ],
        "psr-4": {
            "\\": "bin/",
            "ChurchCRM\\": "ChurchCRM/"
        }
    },
    "repositories": {
        "packagist": {
            "url": "https://packagist.org",
            "type": "composer"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
